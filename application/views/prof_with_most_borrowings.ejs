<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title>Professors With Most Borrowings</title>
  <style>
    body {
      font-family: Arial, sans-serif;
    }
    
    .container {
      max-width: 550px;
      margin: 0 auto;
      margin-top: 100px;
      padding: 20px;
      border: 1px solid #ccc;
      border-radius: 5px;
      background-color: white;
    }
    
    h1 {
      text-align: center;
    }
    
    table {
      width: 100%;
      border-collapse: collapse;
    }
    
    th, td {
      padding: 8px;
      text-align: left;
      border-bottom: 1px solid #ddd;
    }
    
    th {
      background-color: #f2f2f2;
    }
  </style>
</head>
<body>
  <div class="container">
    <h1>Professors With Most Borrowings</h1>
    <table>
      <thead>
        <tr>
          <th>Professor</th>
          <th>Borrowings</th>
        </tr>
      </thead>
      <tbody>
        <% professors.forEach((prof_with_most_borrowings,_) => { %>
          <tr>
            <td><%= prof_with_most_borrowings.stud_prof_id %></td>
            <td><%= prof_with_most_borrowings.num_borrowed %></td>
          </tr>
        <% }) %>
      </tbody>
    </table>
  </div>
</body>
</html>
